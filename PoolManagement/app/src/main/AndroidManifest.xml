<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kapil.poolmanagement">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launche"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launche_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".forgotpassword"></activity>
        <activity android:name=".allusers" />
        <activity android:name=".visitor" />
        <activity android:name=".payment" />
        <activity android:name=".User" />
        <activity android:name=".checkin" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>